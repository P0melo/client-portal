<div class="modal fade" id="edit_airport_transfer_modal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title">Edit Daily Transfer Request</h4>
            </div>
            <div class="modal-body">
                <div class="col-xs-12">

                    <div class="col-lg-12 form-group" id="inboundDate_div">
                        <label class="col-xs-2" style="padding:0;">Inbound Date</label>
                        <span class="col-xs-1" style="font-weight: bold">:</span>
                        <span class="col-xs-3" style="padding:0;"><input type="text" id="e_inboundDate" class="form-control" autocomplete="off"/></span>

                    </div>

                    <div class="col-lg-12 form-group" id="OutboundDate_div">
                        <label class="col-xs-2" style="padding:0;">Outbound Date</label>
                        <span class="col-xs-1" style="font-weight: bold">:</span>
                        <span class="col-xs-3" style="padding:0;"><input type="text" id="e_outboundDate" class="form-control" autocomplete="off"/></span>
                    </div>
                    <div class="col-lg-12 form-group" id="label_attachment_div">
                        <label>Flight Details: </label>
                    </div>
                    <div class="col-lg-12 form-group" id="attachment_div" style="text-align:center">
                        <span style="display: inline-block;" id="e_attachment"></span>
                    </div>

                    <div class="col-xs-6 form-group" style="padding-left: 0px; padding-right: 0px;">
                        <div class="col-xs-12">
                            <label for="InputFile" class="control-label">New Attachment</label>
                            <div class="col-xs-12 form-control">
                                <input type="file" id="e_flight_details_inputFile">

                            </div>
                        </div>
                        <input type="text" id="e_flight_details_output" hidden="">
                    </div>

                    <div class="col-lg-12 form-group" id="notes_div">
                        <label>Remarks: </label>
                        <textarea class="form-control" rows="5" id="e_notes"></textarea>
                    </div>
                </div>
                &nbsp;
            </div>
            <div class="modal-footer">
                <button type="button" id="e_save_transportation_btn" class="btn btn-default pull-right">Submit</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="edit_daily_transfer_modal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title">Edit Daily Transfer Request</h4>
            </div>
            <div class="modal-body">

                <div class="box box-solid">
                    <div id="e_transportation" class="box-body">
                        <div class="col-xs-12 form-group" style="padding-left: 0px; padding-right: 0px;">
                            <div class="col-xs-12">
                                <label for="e_transportation_name" class="control-label">Vehicle</label>
                                <select id="e_transportation_vehicle" class="form-control">
                                    @for (var i = 0; i < Convert.ToInt32(System.Configuration.ConfigurationManager.AppSettings["numberOfVehicles"]); i++)
                                    {
                                        <option value="@System.Configuration.ConfigurationManager.AppSettings["vehicle" + (i + 1)]">
                                            @System.Configuration.ConfigurationManager.AppSettings["vehicle" + (i + 1)]
                                        </option>
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="e_daily_transfer col-xs-12 form-group" style="padding-left: 0px; padding-right: 0px;">
                            <div class="col-xs-12">
                                <label for="e_transportation_destination" class="control-label">Daily Transfer</label>
                            </div>
                            <div class="col-xs-12">
                                <label class="radio-inline"><input type="radio" name="e_dt_optradio" id="e_dt_optradio_1" value="1" checked>One-way</label>
                            </div>
                            <div class="col-xs-12">
                                <label class="radio-inline"><input type="radio" name="e_dt_optradio" id="e_dt_optradio_2" value="2">Round-trip</label>
                            </div>

                        </div>
                        <div class="col-xs-12 form-group e_one_trip_details" style="padding-left: 0px; padding-right: 0px;">
                            <label for="e_transportation_name" class="col-md-12 control-label">First Pick-up/Drop-off point<span class="mandatory">*</span></label>
                            <div class="col-xs-12 col-md-12 col-lg-12 ">
                                <label for="from" class="col-md-12 control-label" style="padding:0; font-size: smaller">From<span class="mandatory">*</span></label>
                                <input id="e_pickup_input" type="text" class="form-group form-control" style="border-radius: 3px;" />
                                <label for="to" class="col-md-12 control-label" style="padding:0; font-size: smaller">To<span class="mandatory">*</span></label>
                                <input id="e_dropoff_input" type="text" class="form-group form-control" style="border-radius: 3px;" />
                            </div>
                            <div class="col-xs-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="e_pick_up_date" placeholder="Choose date">
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-clock-o"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="e_pick_up_time" placeholder="Choose time">
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-12 form-group e_two_trip_details" style="padding-left: 0px; padding-right: 0px; display: none">
                            <label for="e_transportation_name" class="col-md-12 control-label">Second Pick-up/Drop-off point<span class="mandatory">*</span></label>

                            <div class="col-xs-12 col-md-12 col-lg-12 ">
                                <label for="from" class="col-md-12 control-label" style="padding:0; font-size: smaller">From<span class="mandatory">*</span></label>
                                <input id="e_2ndpickup_input" type="text" class="form-group form-control" style="border-radius: 3px;" disabled />
                                <label for="to" class="col-md-12 control-label" style="padding:0; font-size: smaller">To<span class="mandatory">*</span></label>
                                <input id="e_2nddropoff_input" type="text" class="form-group form-control" style="border-radius: 3px;" disabled />
                            </div>
                            <div class="col-xs-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="e_pick_up_date_2" placeholder="Choose date" disabled>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-clock-o"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="e_pick_up_time_2" placeholder="Choose time">
                                </div>
                            </div>
                        </div>
                        <div id="e_daily_transfer_table">
                            <div class="col-xs-12">
                                <button class="btn btn-default" id="add_daily_transfer">Add to table</button>
                            </div>
                            <div class="box-body col-xs-12">
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Type</th>
                                            <th>Pick-up</th>
                                            <th>Drop-off</th>
                                            <th>Date and Time</th>
                                            <th>2nd Pick-up</th>
                                            <th>2nd Drop-off</th>
                                            <th>Date and Time</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <hr />
                            </div>
                        </div>
                        <div class="col-xs-12 form-group" style="display:inline-block; padding-left: 0px; padding-right: 0px;">
                            <label for="transportation_details" class="col-md-2 control-label">Remarks</label>
                            <textarea class="form-control" rows="5" id="e_transportation_details"></textarea>
                        </div>

                        <div class="col-md-12 col-lg-12 form-group">
                            <button type="button" id="e_save_transportation_btn" class="btn btn-default pull-right">Submit</button>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts{
    @Scripts.Render("~/bundles/transportation")
}


